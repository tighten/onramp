{
    "name": "tightenco/onramp",
    "type": "project",
    "description": "Onramp to Laravel.",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "bugsnag/bugsnag-laravel": "^2.22",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "inspheric/nova-url-field": "^1.4",
        "kg-bot/laravel-localization-to-vue": "^1.6",
        "laravel/framework": "^8.50",
        "laravel/nova": "^3.22",
        "laravel/slack-notification-channel": "^2.3",
        "laravel/tinker": "^2.5",
        "mrmonat/nova-translatable": "^2.0",
        "spatie/laravel-translatable": "^4.0",
        "tightenco/nova-releases": "^1.0",
        "tightenco/suggested-resources-shortcuts": "*",
        "tightenco/ziggy": "^1.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.10",
        "beyondcode/laravel-dump-server": "^1.7",
        "brianium/paratest": "^6.3",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "filp/whoops": "^2.13",
        "friendsofphp/php-cs-fixer": "^2.18",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "tightenco/tlint": "^6.0",
        "wulfheart/pretty_routes": "^0.1.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/SuggestedResourcesShortcuts"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
