{
    "name": "tightenco/onramp",
    "type": "project",
    "description": "Onramp to Laravel.",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "path",
            "url": "./nova-components/SuggestedResourcesShortcuts"
        },
        {
            "type": "vcs",
            "url": "https://github.com/astrotomic/nova-optional.git"
        }
    ],
    "require": {
        "php": "^7.3.0",
        "astrotomic/nova-optional": "dev-main",
        "bugsnag/bugsnag-laravel": "^2.0",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "inspheric/nova-url-field": "^1.4",
        "kg-bot/laravel-localization-to-vue": "^1.6",
        "laravel/framework": "^7.0",
        "laravel/nova": "^3.10",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/tinker": "^2.0",
        "mrmonat/nova-translatable": "^2.0",
        "spatie/laravel-translatable": "^4.2",
        "tightenco/nova-releases": "^0.2.5",
        "tightenco/suggested-resources-shortcuts": "*",
        "tightenco/tlint": "^5.0",
        "tightenco/ziggy": "^0.8.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.7",
        "beyondcode/laravel-dump-server": "^1.0",
        "facade/ignition": "^2.0",
        "filp/whoops": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.17",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "inspheric/nova-url-field",
                "mrmonat/nova-translatable",
                "tightenco/nova-releases",
                "tightenco/suggested-resources-shortcuts"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "pre-install-cmd": [
            "[ -d nova/src/Actions ] || mkdir -p nova/src/Actions",
            "[ -f nova/composer.json ] || echo '{\"name\":\"laravel/nova\",\"description\":\"\",\"version\":\"3.9999.0\",\"autoload\":{\"psr-4\":{\"Laravel\\\\\\Nova\\\\\\\":\"src/\"}}}' > nova/composer.json",
            "[ -f nova/src/Actions/Actionable.php ] || echo '<?php namespace Laravel\\Nova\\Actions; trait Actionable{}' > nova/src/Actions/Actionable.php"
        ],
        "pre-update-cmd": [
            "[ -d nova/src/Actions ] || mkdir -p nova/src/Actions",
            "[ -f nova/composer.json ] || echo '{\"name\":\"laravel/nova\",\"description\":\"\",\"version\":\"3.9999.0\",\"autoload\":{\"psr-4\":{\"Laravel\\\\\\Nova\\\\\\\":\"src/\"}}}' > nova/composer.json",
            "[ -f nova/src/Actions/Actionable.php ] || echo '<?php namespace Laravel\\Nova\\Actions; trait Actionable{}' > nova/src/Actions/Actionable.php"
        ]
    }
}
